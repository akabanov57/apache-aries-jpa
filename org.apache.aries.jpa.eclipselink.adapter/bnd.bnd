	osgi.core,\
	javax.persistence-api,\
	org.ops4j.pax.logging.pax-logging-api,\
	org.eclipse.persistence.core,\
	javax.transaction-api
-sources: false
Private-Package: \
	org.apache.aries.jpa.eclipselink.adapter,\
	org.apache.aries.jpa.eclipselink.adapter.platform
Bundle-Activator: org.apache.aries.jpa.eclipselink.adapter.Activator
Import-Package: \
	org.osgi.framework;version='[1.6.0,2.0.0)',\
	javax.persistence.spi;version='[2.0.0,3.0.0)',\
	javax.persistence;version='[2.0.0,3.0.0)',\
	org.eclipse.persistence.transaction;version='[2.6.0,3.0.0)',\
